<%- include ('header') -%>

<main class="contact-section">
    <% if(pageData.length){ %>
        <section class="join-section-bg" style="background-image: url(<%= pageData[0].imagesJoinOurTeamSlider[0] %>)">
            <div class="container">
                <h1 class="text-center"><%= pageData[0].textJoinOurTeamSlider %></h1>
            </div>
        </section>
        <section class="section py-5">
            <div class="container container-1730">
                <h2 class="text-center mb-lg-5 "><%= pageData[0].joinOurTeamWorkUs %></h2>
                <div class="row mb-5 justify-content-between">
                    <div class="col-12 col-md-3">
                        <div class="title"><%= pageData[0].joinOurCol1Title %></div>
                        <div class="text">
                            <p>
                                <%= pageData[0].joinOurCol1Text %>
                            </p>
                        </div>
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="title"><%= pageData[0].joinOurCol2Title %></div>
                        <div class="text">
                            <p>
                                <%= pageData[0].joinOurCol2Text %>
                            </p>
                        </div>
                    </div>
                    <div class="col-12 col-md-3">
                        <div class="title"><%= pageData[0].joinOurCol3Title %></div>
                        <div class="text">
                            <p>
                                <%= pageData[0].joinOurCol3Text %>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section>
            <div class="container">
                <div class="row justify-content-between align-items-center flex-wrap pb-5 pt-lg-5 px-md-5 my-lg-5">
                    <div class="col-12 col-md-7 ">
                        <h2 class="mb-3"><%= pageData[0].joinOurTeamPartnersTitle %></h2>
                        <div class="pr-5">
                            <div class="info-text mb-5 mb-md-0">
                                <%= pageData[0].joinOurTeamPartners %>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-md-5 ">
                        <div class="rose-border">
                            <img src=<%= pageData[0].imagesJoinOurTeamSlider[1] %> alt="" class="brand-img">
                        </div>
                    </div>
                </div>
            </div>
        </section>
    <% } %>
</main>
<%- include ('footer') -%>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
<script src="js/jquery.js"></script>
<script src="js/bootstrap/bootstrap.min.js"></script>
<script src="js/script.js"></script>

</body>
</html>
